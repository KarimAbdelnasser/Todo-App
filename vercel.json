{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "PUBLIC_URL": "/",
        "VIEWS_DIRECTORY": "views"
    },
    "public": true,
    "ignore": ["views/**/*.js", "views/**/*.css"]
}
